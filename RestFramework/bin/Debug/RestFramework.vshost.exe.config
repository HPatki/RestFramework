<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <!-- size in bytes-->
      <add key="maxPayLoad" value="1024"/>
      <!-- file upload max size is 10MB. Anything more than that has to be done in chunks
      handled explicitly by sending application -->
      <add key="maxFilePayLoad" value ="10000000"/>
    </appSettings>
</configuration>